int foo(int x)
{
	int result; 
	result = 0; 
	
	if (!(x == -1)) 
	{
		if (x < 1) 
		{
			print x; 
			result = foo(x + 1); 
		} 
		else if (x < 100) 
		{ 
			print x; 
			result = foo(x + 11); 
		} 
		else 
		{ 
			result = x; 
		} 
	} 
	return result;
} 

int main()
{ 
	int x; 
	x = 0; 
	foo(x); 
	return 0; 
	
} 
