int isodd(int x, int y, int z) {
	int result;
	result = 0;
	
	if (x % z == y) {
		result = 1;
	} else {
		result = 0;
	}
	return result;
}

int iseven(int x, int y, int z) {
	int result;
	
	if (x % z != y) {
		result = 1;
	} else {
		result = isodd(x, y, z);
	}
	return result;
}

int main() {
	int x;
	int y;
	int z;
	int result;
	x = 0;
	y = 1;
	z = 2;
	while (10 > x) {
		result = isodd(x, y, z);
		print result;
		result = iseven(x, y, z);
		print result;
		x = x + y;
	}
	return 0;
}
